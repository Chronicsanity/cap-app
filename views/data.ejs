<!DOCTYPE html>
<html>
    <div class="text-center center-div" id="login">
        <div class="container w-25 border py-5">
            <div class="title pb-5">
                <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
                
<head>
	
</head>

<body>
  
    <table class="table">
        <thead>
          <tr>
           
            <th>Name</th>
           
            <th>Password </th>
          </tr>
        </thead>
        <tbody>
           
            <%const result = [] %>
            <% for (var i = 0; i < user.length; i++) { %>
            
                <tr>
                    <td><%= user[i].username %></td>
                    <td><%= user[i].password %> </td>
                </tr>
            <%  } %>
            <div>
                <p>Add New User</p>
            </div>
            <form action="/data" method="POST" class="pt-3">
                <div class="form-group">
                    <input type="username" class="form-control" placeholder="username" name="username">
                    <small class="form-text text-muted text-left">Register Username</small>
                </div>
                <div class="form-group">
                    <input type="password" class="form-control" name="password" placeholder="password">
                </div>
                <button type="submit" class="btn btn-success rounded-pill">Submit</button>
            </form>

          </tbody>
        </table>


</body>
            </div>
        </div>
        </div>
</html>
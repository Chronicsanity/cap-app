<!DOCTYPE html>
    <head lang="en">
        <meta charset="UTF-8">
        <title>1. Basic</title>
        <meta http-equiv="Pragma" content="no-cache">
        <meta http-equiv="Expires" content="-1">
    </head>

    <body>
        <div class="code-html">
            <table>
                <tbody>
                    <tr>
                        <td><div id="timepicker-selectbox"><script>tpSelectbox</script></div></td>
                    </tr>
                    <tr>
                        <td><div id="timepicker-spinbox"><script>tpSpinbox</script></div></td>
                    </tr>
                </tbody>
            </table>
        </div>


        <script type="text/javascript" class="code-js">
            var tpSelectbox = new tui.TimePicker('#timepicker-selectbox', {
                initialHour: 3,
                initialMinute: 3,
                disabledHours: [1, 2, 14],
                inputType: 'selectbox'
            });
            var tpSpinbox = new tui.TimePicker('#timepicker-spinbox', {
                initialHour: 15,
                initialMinute: 33,
                disabledHours: [1, 2, 13, 14],
                inputType: 'spinbox'
            });
        </script>
    </body>
</html>
<h1>Main page</h1>

<div class="text-center center-div mt-5" id="login">
    <div class="container w-25 border py-5 mt-5">
        <h3>Welcome to Express Dashboard</h3>
        <body>
        
          <html>
            <table id="table" >  
                <thead>  
                    <tr>  
                        <th>Username</th>  
                        <th>Password</th>  
                    </tr>  
                </thead>  
                 <tbody>  
                 <% print(forEach(function (user)) { %>
                    <tr>  
                        <td><%= user.username %></td>  
                        <td><%= user.password %></td>
                    </tr>                                   
                 <% }) %>
                 </tbody>
            </table>   
            </html>
        </body>
        

        <h5><%= locals.user ? user : "User" %></h5>
        <a href="/controllers/auth.controller">Logout</a>
    </div>
</div>

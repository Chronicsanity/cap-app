<DOCTYPE html>
<html lang="en">

    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        
    </head>

<div class="text-center center-div mt-5" id="dashboard">
    <div class="container w-25 border py-5 mt-5">
        <h3>Welcome to Express Dashboard</h3>
        <body>
          
        <div class="schedule-table">
            
            <table id="table" >  
                <thead>  
                    <tr>  
                        <th>Username</th>  
                        <th>Password</th>  
                    </tr>  
                </thead>  
                 <tbody>  
                    <tr>
                       <%- await data.forEach(users) => {(  %> 
                            <td><%- users.Username, %></td> 
                            <td><%- users.Password )}; %></td>
                 </tr>
                   
                 </tbody>
            </table>   
            </div>
        </script>
            
        </body>
    

        <h5><%= locals.user ? user + " User" %></h5>
        <a href="/controllers/auth.controller">Logout</a>
    </div>
</div>
